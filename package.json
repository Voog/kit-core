{
  "name": "kit-core",
  "version": "0.1.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all": "npm run build && npm run test",
    "prebuild": "echo Rebuilding ./index.js",
    "build": "rollup -m inline -c",
    "postbuild": "echo `echo Rebuilt ./index.js $(date +\"[%H:%M:%S]\")` && touch ../kit-cli/src/kit.js",
    "pretest": "echo Running tests",
    "test": "mocha ./test/test.js || true",
    "watch": "watch 'npm run all' ./src ./test",
    "watch:build": "watch 'npm-run-all build test' ./src",
    "watch:test": "watch 'npm run test' ./test"
  },
  "author": "Mikk Pristavka",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.3.1",
    "highland": "^2.7.1",
    "lodash": "^4.5.0",
    "mime-db": "^1.22.0",
    "mime-type": "^3.0.4",
    "request": "^2.69.0",
    "voog": "git+https://github.com/Voog/voog.js.git"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "mocha": "^2.4.5",
    "mocha-sinon": "^1.1.5",
    "nock": "^8.0.0",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-json": "^2.0.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "watch": "^0.17.1"
  }
}
